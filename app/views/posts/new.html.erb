

<div class="container-fluid">
  <%= form_for :post, url: posts_path(@post) do |f| %>
  <%= f.text_field :title, placeholder: 'Title' %><br><br>
  <%= f.text_area :body, placeholder: 'how you feel'%><br>
  <%= f.submit %>
  <% end %>
  <%= link_to 'Back', root_path %>
</div>


<div class="show-post"></div>

<div class="results"></div>
